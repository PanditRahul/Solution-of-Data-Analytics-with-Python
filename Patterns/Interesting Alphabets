from os import *
from sys import *
from collections import *
from math import *

from os import *
from sys import *
from collections import *
from math import *

## Read input as specified in the question.
## Print output as specified in the question.
n=int(input())
for i in range(n,0,-1):
    for j in range(i,n+1):
        print(chr(j+64),end="")
    print("")

